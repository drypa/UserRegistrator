<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RegistrationService"
                   namespace="WcfCommon.Domain">
  <class name="User" table="[User]">
    <id name="Id">
      <generator class="guid" />
    </id>
    <property name="LastName" type="System.String" length="100" />
    <property name="FirstName" type="System.String" length="100" />
    <property name="CityId" />
  </class>

</hibernate-mapping>
